{
  "apps": [
    {
      "name": "ai-portfolio",
      "script": "npm",
      "args": "start",
      "cwd": "/opt/bitnami/apache/htdocs/ai-portfolio",
      "instances": 1,
      "exec_mode": "fork",
      "watch": false,
      "max_memory_restart": "1G",
      "env": {
        "NODE_ENV": "production",
        "PORT": 3001
      },
      "error_file": "./logs/err.log",
      "out_file": "./logs/out.log",
      "log_file": "./logs/combined.log",
      "time": true
    }
  ]
}
